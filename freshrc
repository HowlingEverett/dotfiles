# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions/\*
#   fresh jasoncodes/dotfiles shell/aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin

# Load vim plugins and config
fresh zshenv --file
fresh zshrc --file
fresh gitignore_global --file
fresh vimrc.plug --file
fresh vimrc.plug --file=~/.config/nvim/init.vim.plug
fresh vimrc --file
fresh vimrc --file=~/.config/nvim/init.vim
